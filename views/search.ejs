<% include theme/header %> 
<div class="call-to-action">
    <div class="container-fluid">
        <div class="row align-items-center">
            <div class="col-md-6">
                <h1>Kết quả tìm kiếm</h1>
            </div>
            <div class="col-md-6">
                <!-- <a href="tel:0123456789">+012-345-6789</a> -->
            </div>
        </div>
    </div>
</div>

<div class="container" style="margin-top:50px;">
    <div class="row">
        <%length=data.length%>
        <%if(length==0){%>
            <div class="loisp">
                Sản phẩm tìm kiếm không tồn tại!
            </div>
        <%}%>
        <%data.forEach(function(motphantu){%>
        <%price_tiki=motphantu.price_book%>
        <%list_price_tiki=motphantu.list_price%>
        <%discount_tiki=motphantu.discount_rate%>

        <%price_fahasa =motphantu.list_produce_fahasa[0].price_book%>
        <%list_price_fahasa=motphantu.list_produce_fahasa[0].list_price%>
        <%discount_fahasa=motphantu.list_produce_fahasa[0].discount_rate%>

        <%price_lazada =motphantu.list_produce_lazada[0].price_book%>
        <%list_price_lazada=motphantu.list_produce_lazada[0].list_price%>
        <%discount_lazada=motphantu.list_produce_lazada[0].discount_rate%>
        
        
        <div class="col-xs-12 col-sm-6 col-md-3">
            <div class="col-item">
                <div class="post-img-content">
                    <img src="<%=motphantu.img_book%>" class="img-responsive" />
                    <% if(price_tiki <=price_fahasa && price_tiki<=price_lazada) {%>
                        <span class="round-tag">-<%=discount_tiki%>%</span>
                    <%}%>
                    <% if(price_fahasa <price_tiki && price_fahasa<price_lazada) {%>
                        <span class="round-tag">-<%=discount_fahasa%>%</span>
                    <%}%>
                    <% if(price_lazada <price_fahasa && price_lazada<price_tiki) {%>
                        <span class="round-tag">-<%=discount_lazada%>%</span>
                    <%}%>
                </div>
                <div class="info">
                    <div class="name">
                        <a href="/chi-tiet/<%=motphantu.name_unicode%>.<%=motphantu.id_book%>.html"><span><%=motphantu.name_book%></span>
                        </a>
                    </div>
                    <% if(price_tiki <=price_fahasa && price_tiki <=price_lazada) {%>
                        <div class="row">
                            <div class="price col-md-6">
                                <h5 class="price-text-color "><span class='number'><%=price_tiki%></span>₫</h5>
                            </div>
                            <div class="price col-md-6">
                                <span class="list-price-text"><span
                                        class='number text'><%=list_price_tiki%></span>₫</span>
                            </div>
                            <div class="separator clear-left">
                                <p class="btn-add">
                                    <img src="https://img.websosanh.vn/v2/users/logo/images/53mjoSwxCgpn.jpg?compress=85&width=50&height=30"
                                        alt="Tiki">
                                </p>
                                <p class="btn-details">
                                    <a href="/category/<%=motphantu.category_unicode%>.<%=motphantu.cat_id%>.html"
                                        class="hidden-sm"><%=motphantu.category%></a>
                                </p>
                            </div>
                        </div>
                    <%}%>
                    <% if(price_fahasa <price_tiki && price_fahasa <price_lazada) {%>
                        <div class="row">
                            <div class="price col-md-6">
                                <h5 class="price-text-color "><span class='number'><%=price_fahasa%></span>₫</h5>
                            </div>
                            <div class="price col-md-6">
                                <span class="list-price-text"><span
                                        class='number text'><%=list_price_fahasa%></span>₫</span>
                            </div>
                            <div class="separator clear-left">
                                <p class="btn-add">
                                    <img src="https://jobseekers.vn/wp-content/uploads/2019/05/Quy-Chuan-Logo.png"lt="fahasa" class='img_fahasa'>
                                </p>
                                <p class="btn-details">
                                    <a href="/category/<%=motphantu.category_unicode%>.<%=motphantu.cat_id%>.html"
                                        class="hidden-sm"><%=motphantu.category%></a>
                                </p>
                            </div>
                        </div>
                    <%}%>
                    <% if(price_lazada <price_tiki && price_lazada <price_fahasa) {%>
                        <div class="row">
                            <div class="price col-md-6">
                                <h5 class="price-text-color "><span class='number'><%=price_lazada%></span>₫</h5>
                            </div>
                            <div class="price col-md-6">
                                <span class="list-price-text"><span
                                        class='number text'><%=list_price_lazada%></span>₫</span>
                            </div>
                            <div class="separator clear-left">
                                <p class="btn-add">
                                    <img src="/images/lazada.png"lt="lazada" class='img_lazada'>
                                </p>
                                <p class="btn-details">
                                    <a href="/category/<%=motphantu.category_unicode%>.<%=motphantu.cat_id%>.html"
                                        class="hidden-sm"><%=motphantu.category%></a>
                                </p>
                            </div>
                        </div>
                    <%}%>

                   

                </div>
            </div>
        </div>
        <%})%>
    </div>
</div>
<script>
    let x = document.querySelectorAll(".number");
    for (let i = 0, len = x.length; i < len; i++) {
        let num = Number(x[i].innerHTML)
                  .toLocaleString('vi');
        x[i].innerHTML = num;
        
    }
</script> 
</script> 
<link href="/stylesheets/1.css" rel="stylesheet">
<% include theme/footer %>  